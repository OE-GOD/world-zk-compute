{
  "name": "@worldzk/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for World ZK Compute - Decentralized ZK Proving Network",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zk",
    "zero-knowledge",
    "risc-v",
    "proof",
    "blockchain",
    "web3",
    "risc-zero"
  ],
  "author": "World ZK Compute",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/OE-GOD/world-zk-compute.git",
    "directory": "sdk/typescript"
  },
  "bugs": {
    "url": "https://github.com/OE-GOD/world-zk-compute/issues"
  },
  "homepage": "https://github.com/OE-GOD/world-zk-compute#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
